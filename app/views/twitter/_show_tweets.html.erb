<% tweets.each do |tweet| %>
  <div>
    <img class="img-circle" src= '<%= image_path(tweet.user.profile_image_url_https.to_s.gsub('_normal','')) %>' alt="" style="width: 50px;height: 50px;">
  </div>

  <div>
    <h4> <%= tweet.user.name %></h4>
  </div>
  <div class="timeline-body">
      <p> <%= tweet.text%> </p>
      <i> Retweets: <%= tweet.retweet_count %> </i>
      <i> Likes: <%= tweet.favorite_count %> </i>
  </div>
  <br>
<% end %>
